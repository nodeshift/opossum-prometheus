{
  "name": "opossum-prometheus",
  "version": "0.3.0",
  "description": "Prometheus metrics for opossum circuit breaker",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "pretest": "npm run lint",
    "test": "nyc tape test/*.js | tap-spec",
    "prerelease": "npm run test",
    "release": "standard-version -s -a",
    "lint": "standardx test/*.js index.js"
  },
  "support": {
    "target": "LTS",
    "response": "REGULAR-7",
    "backing": "COMPANY"
  },
  "keywords": [
    "circuit breaker",
    "fail fast",
    "prometheus",
    "metrics"
  ],
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "husky": "^6.0.0",
    "nyc": "^15.1.0",
    "opossum": "^5.1.3",
    "standard-version": "^9.3.0",
    "standardx": "^7.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.1.1"
  },
  "dependencies": {
    "prom-client": "^12.0.0"
  },
  "engines": {
    "node": "^16 || ^14 || ^12"
  }
}
